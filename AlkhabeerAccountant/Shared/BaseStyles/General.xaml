<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib" >
    <!--=========== Icon Size================ -->
    <sys:Double x:Key="IconSize-1">20</sys:Double>
    <sys:Double x:Key="IconSize-2">25</sys:Double>
    <sys:Double x:Key="IconSize-3">30</sys:Double>

    <!--=========== button height================ -->
    <sys:Double x:Key="ButtonHeight-1">40</sys:Double>
    <sys:Double x:Key="ButtonHeight-2">45</sys:Double>
    <sys:Double x:Key="ButtonHeight-3">50</sys:Double>
    <!--=========== Border Thick================ -->
    <Thickness x:Key="BorderThick-1">1</Thickness>
    <Thickness x:Key="BorderThick-2">2</Thickness>
    <Thickness x:Key="BorderThick-3">3</Thickness>

    <Thickness x:Key="ContentPadding">15</Thickness>
    <Thickness x:Key="ContentMargin">10</Thickness>
    <!--============cornerRadius============-->
    <CornerRadius x:Key="ButtonCornerRadius">6</CornerRadius>
    <CornerRadius x:Key="InputCornerRadius">4</CornerRadius>
    <CornerRadius x:Key="MainCardCornerRadius">10</CornerRadius>
    <CornerRadius x:Key="WindowCornerRadius">10</CornerRadius>


</ResourceDictionary>